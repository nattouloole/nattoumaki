<div class="container">  
  <div class="row">  
    <h2>会員一覧</h2>
    
      <table class="table table-hover table-inverse">
        
        <thead>
          <tr>
            <th>会員ID</th>
            <th>氏名</th>
            <th>メールアドレス</th>
            <th>ステータス</th>
          </tr>
        </thead>
    
      <tbody>
        <% @admins.each do |admin| %>
          <tr>
            <td></td>
            <td><%= link_to admin.name, %></td>
            <td><%= admin.email %></td>
          </tr>
      </tbody>
        
